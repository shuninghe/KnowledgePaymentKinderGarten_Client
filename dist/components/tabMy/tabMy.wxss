.tabMy .btn {
  background-color: #fff;
  border-radius: none;
  padding: 0;
  margin: 0;
  line-height: 1;
  border: none;
}
.tabMy .btn.userinfo-wrap {
  background-color: transparent;
}
.tabMy .btn::after {
  border: none;
}
.my-info {
  height: 200rpx;
  padding-left: 34rpx;
  background-image: url('https://woxue-dev.oss-cn-beijing.aliyuncs.com/1587193457554.5542tabMy-bgTree.png');
  background-repeat: no-repeat;
  background-size: 200rpx 200rpx;
  background-position: 550rpx 0;
  background-color: #fff;
}
.my-info .avatar {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin-right: 28rpx;
}
.my-info .avatar .img {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.my-info .info .name {
  max-width: 350rpx;
  font-size: 34rpx;
  color: #000000;
  font-weight: bold;
}
.my-info .info .edit {
  height: 50rpx;
  margin-left: 42rpx;
  font-size: 22rpx;
}
.my-info .info .edit-icon {
  width: 22rpx;
  height: 20rpx;
  margin-right: 6rpx;
  font-size: 22rpx;
  color: #282828;
}
.my-info .info .vip-wrap {
  margin-top: 22rpx;
}
.my-info .info .vip {
  padding: 0 20rpx;
  height: 35rpx;
  background: #ffffff;
  border: 1px solid #FFCA2C;
  border-radius: 36rpx;
  margin-right: 22rpx;
}
.my-info .info .vip-icon {
  width: 28rpx;
  height: 26rpx;
  margin-right: 7rpx;
}
.my-info .info .vip-name {
  display: inline-block;
  max-width: 200rpx;
}
.my-info .info .txt {
  font-size: 22rpx;
  color: #202020;
}
.tab {
  height: 146rpx;
  margin-top: 14rpx;
  background-color: #fff;
  text-align: center;
  border-bottom: 2rpx solid #f3f3f3;
}
.tab .tab-icon {
  width: 55rpx;
  height: 55rpx;
}
.tab .text {
  font-size: 22rpx;
  color: #282828;
  line-height: 1.2;
}
.list {
  padding-left: 43rpx;
  background-color: #fff;
}
.list .item {
  height: 100rpx;
  padding-right: 46rpx;
  border-bottom: 2rpx solid #f3f3f3;
  font-size: 28rpx;
}
.list .item .icon {
  width: 50rpx;
  height: 50rpx;
  margin-right: 28rpx;
}
.list .item .right {
  width: 20rpx;
  height: 26rpx;
}
