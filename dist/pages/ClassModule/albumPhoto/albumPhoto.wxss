page {
  width: 100%;
  height: 100%;
  background: #fff;
}
.albumPhoto {
  padding: 40rpx 46rpx 0 36rpx;
  display: flex;
  flex-wrap: wrap;
}
.albumPhoto .list {
  margin: 0 13.5rpx 40rpx;
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.albumPhoto .list .delIcon {
  width: 45rpx;
  height: 45rpx;
  position: absolute;
  top: -10rpx;
  right: -10rpx;
}
.albumPhoto .list .albmImg {
  width: 140rpx;
  height: 140rpx;
  border-radius: 10rpx;
}
.albumPhoto .addAlbm {
  width: 140rpx;
  height: 140rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 13.5rpx 40rpx;
  border-radius: 10rpx;
  background: #f6f6f6;
}
.albumPhoto .addAlbm .icon {
  width: 70rpx;
  height: 70rpx;
}
.albumPhoto .delAlbm {
  width: 140rpx;
  height: 140rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 13.5rpx 40rpx;
  border-radius: 10rpx;
  background: #f6f6f6;
}
.albumPhoto .delAlbm .icon {
  width: 70rpx;
  height: 70rpx;
}
.mask {
  background: #fff;
  width: 100%;
  height: 100%;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 100;
  display: flex;
  justify-content: center;
  align-items: center;
}
.mask .maskImg {
  width: 100%;
  height: auto;
}
.mask .maskVideo {
  width: 100%;
}
/*遮罩层*/
.modal-mask {
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 9999;
}
/*中间内容*/
.main-content {
  box-sizing: border-box;
  padding: 48rpx 0;
  overflow-y: hidden;
}
/*遮罩内容*/
.modal-content-timeout {
  display: flex;
  flex-direction: column;
  width: 80%;
  background-color: #fff;
  border-radius: 10rpx;
  text-align: center;
  overflow-y: hidden;
}
/*底部按钮*/
.modal-footer {
  display: flex;
  justify-content: center;
  margin-top: 12rpx;
  margin-bottom: 48rpx;
}
.cancel-btn,
.confirm-btn {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 210rpx;
  height: 74rpx;
  border-radius: 40rpx;
  text-align: center;
  font-size: 30rpx;
  padding: 12rpx;
}
.cancel-btn {
  background: #EEEEEE;
  color: #7D7D7D;
  margin-right: 24rpx;
}
.confirm-btn {
  background: #107E7D;
  color: #FFFFFF;
  margin-left: 24rpx;
}
/* 模态对话框 */
.main-content .title {
  font-size: 36rpx;
  color: #333333;
  margin-bottom: 40rpx;
  font-weight: bold;
}
.main-content .txt {
  font-size: 32rpx;
  color: #4A4A4A;
}
