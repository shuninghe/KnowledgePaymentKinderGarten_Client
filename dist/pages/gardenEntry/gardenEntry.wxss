.gardenEntry {
  min-height: 100%;
  background-color: #fff;
  padding: 60rpx 0;
}
.gardenEntry .mini-logo {
  display: block;
  width: 120rpx;
  height: 120rpx;
  margin: 0 auto 32rpx;
}
.gardenEntry .logo-info {
  display: block;
  width: 431rpx;
  height: 119rpx;
  margin: 0 auto 32rpx;
}
.gardenEntry .logo {
  display: block;
  width: 186rpx;
  height: 186rpx;
  margin: 0 auto;
}
.gardenEntry .content {
  width: 506rpx;
  margin: 62rpx auto 0;
}
.gardenEntry .item {
  padding: 30rpx 0 20rpx 6rpx;
  border-bottom: 2rpx solid #f1f1f1;
}
.gardenEntry .item-icon {
  width: 54rpx;
  height: 54rpx;
  margin-right: 10rpx;
}
.gardenEntry .item-input {
  width: 420rpx;
}
.gardenEntry .item-input.gray {
  color: #888;
}
.gardenEntry .item-input.w250 {
  width: 250rpx;
}
.gardenEntry .item-txt {
  width: 140rpx;
  font-size: 26rpx;
  color: #282828;
}
.gardenEntry .item-txt.default-btn {
  padding: 0;
  margin: 0;
  background: none;
  color: #107e7d;
}
.gardenEntry .item-txt.default-btn::after {
  border: none;
}
.gardenEntry .tip {
  margin: 30rpx 0 20rpx;
}
.gardenEntry .tip-txt {
  font-size: 24rpx;
  color: #48c05f;
}
.gardenEntry .tip-txt.c107e7d {
  color: #107e7d;
}
.gardenEntry .tip-icon {
  width: 20rpx;
  height: 24rpx;
}
.gardenEntry .btn {
  width: 505rpx;
  height: 85rpx;
  background-color: #48c05f;
  border-radius: 8rpx;
  margin: 30rpx auto 0;
  font-size: 28rpx;
  color: #fff;
}
.gardenEntry .btn.c107e7d {
  background-color: #107e7d;
}
.gardenEntry .btn.disabled {
  background-color: #e4e4e4;
}
